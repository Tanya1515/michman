{
  "Type": "fanlight",
  "Description": "Fanlight service",
  "DefaultVersion": "default",
  "Versions": [
    {
      "Version": "default",
      "Description": "Fanlight default version",
      "Configs": [
        {
          "ParameterName": "instance_url",
          "Type": "string",
          "Required": true,
          "Description": "Fanlight frontend vnclient base URL to build site links: <link rel='stylesheet' href='fanlight_instance_url/vnc-toolbar/vnc-toolbar.min.css'>"
        },
        {
          "ParameterName": "desktop_access_url",
          "Type": "string",
          "Required": true,
          "Description": "This parameter (without protocol part and desktop id at the end), is used to construct noVNC rfb connection URL on the web page: (ws|wss)://DesktopAccessURL/desktop_id"
        },
        {
          "ParameterName": "users_add",
          "Type": "string",
          "Required": true,
          "Description": "adds users on startup"
        },
        {
          "ParameterName": "apps_add",
          "Type": "string",
          "Required": true,
          "Description": "list of applications to add on startup"
        },
        {
          "ParameterName": "weblab_name",
          "Type": "string",
          "Required": true,
          "Description": "name of Web Laboratory"
        },
        {
          "ParameterName": "fileshare_ui_ip",
          "Type": "string",
          "Required": true,
          "Description": "fileshare_ui_ip"
        },
        {
          "ParameterName": "nextcloud_nfs_server_ip",
          "Type": "string",
          "Required": true,
          "Description": "NFS server IP"
        }
      ]
    }]
}